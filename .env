# Firebase Project Credentials (Client-side)
# Find these in your Firebase project settings > General
NEXT_PUBLIC_FIREBASE_API_KEY=<YOUR_FIREBASE_API_KEY_HERE>
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=<YOUR_FIREBASE_AUTH_DOMAIN_HERE>
NEXT_PUBLIC_FIREBASE_PROJECT_ID=<YOUR_FIREBASE_PROJECT_ID_HERE>
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=<YOUR_FIREBASE_STORAGE_BUCKET_HERE>
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=<YOUR_FIREBASE_MESSAGING_SENDER_ID_HERE>
NEXT_PUBLIC_FIREBASE_APP_ID=<YOUR_FIREBASE_APP_ID_HERE>

# Firebase Admin SDK Credentials (Server-side)
# Find these in your Firebase project settings > Service accounts > Generate new private key
FIREBASE_PROJECT_ID=<YOUR_FIREBASE_PROJECT_ID_HERE>
FIREBASE_CLIENT_EMAIL=<YOUR_FIREBASE_SERVICE_ACCOUNT_EMAIL_HERE>
# Make sure to format the private key correctly, replacing newlines with \n
FIREBASE_PRIVATE_KEY="<YOUR_FIREBASE_PRIVATE_KEY_HERE>"

# Google Maps API Key (Client-side)
# Find this in your Google Cloud Platform credentials page
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=<YOUR_GOOGLE_MAPS_API_KEY_HERE>

# Cashfree Payment Gateway Credentials (Server-side)
# Find these in your Cashfree dashboard > Developers > API Keys
CASHFREE_APP_ID=<YOUR_CASHFREE_APP_ID_HERE>
CASHFREE_SECRET_KEY=<YOUR_CASHFREE_SECRET_KEY_HERE>
CASHFREE_WEBHOOK_SECRET=<YOUR_CASHFREE_WEBHOOK_SECRET_HERE>
GEMINI_API_KEY=