{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "db:seed": "tsx src/lib/firebase/seed.ts",
    "db:cleanup": "tsx src/lib/firebase/cleanup-test-data.ts",
    "postinstall": "patch-package",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:full": "playwright test tests/e2e/complete-transaction-cycle.spec.ts",
    "test:edge-cases": "playwright test tests/e2e/edge-cases.spec.ts",
    "test:a11y": "playwright test tests/accessibility/a11y.spec.ts",
    "test:performance": "playwright test tests/performance/page-load.spec.ts",
    "test:visual": "playwright test tests/visual/visual-regression.spec.ts --update-snapshots",
    "test:invoice": "playwright test tests/e2e/invoice.spec.ts",
    "test:reviews": "playwright test tests/e2e/reviews.spec.ts",
    "test:all": "playwright test tests/e2e tests/accessibility tests/performance tests/e2e/invoice.spec.ts tests/e2e/reviews.spec.ts",
    "test:debug": "playwright test --debug",
    "test:regression": "playwright test tests/e2e/complete-transaction-cycle.spec.ts tests/e2e/mobile_user_flow.spec.ts",
    "test:report": "playwright show-report",
    "analyze": "cross-env ANALYZE=true npm run build -- --webpack",
    "test:lighthouse": "lhci autorun",
    "test": "jest",
    "test:unit": "jest",
    "predeploy": "tsx scripts/preflight-check.ts",
    "deploy": "firebase deploy"
  },
  "engines": {
    "node": "20"
  },
